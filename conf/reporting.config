timestamp = new java.util.Date().format('yyyy-MM-dd_HH-mm')
trace.overwrite = true
dag.overwrite = true

dag {
    enabled = true
    file    = "${params.publish_dir}/summary/${timestamp}_dag.html"
}

report {
    enabled = true
    file    = "${params.publish_dir}/summary/${timestamp}_report.html"
}

timeline{
    enabled = true
    file    = "${params.publish_dir}/summary/${timestamp}_timeline.html"
}

trace {
    enabled = true
    file    = "${params.publish_dir}/summary/${timestamp}_trace.html"
}
