profiles {
    custom {
        executor {
            queueSize = 50
            submitRateLimit = '10/1min'
        }

        process {
            withLabel: lil_mem {
                clusterOptions = '-A <account> \
                                  -p <partition> \
                                  -q <qos>'
                cpus = 1
            }
            withLabel: med_mem {
                clusterOptions = '-A <account> \
                                  -p <partition> \
                                  -q <qos>'
                cpus = 4
                maxForks = 4
            }
            withLabel: big_mem {
                clusterOptions = '-A <account> \
                                  -p <partition> \
                                  -q <qos>'
                cpus = 8
                maxForks = 2
            }
            withLabel: sup_mem {
                clusterOptions = '-A <account> \
                                  -p <partition> \
                                  -q <qos>'
                cpus = 8
                maxForks = 1
            }

        }
    }

}
